<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:f="urn:import:com.silverpeas.mobile.client.components">

	<ui:with field='msg' type='com.silverpeas.mobile.client.apps.contacts.resources.ContactsMessages' />

		
	<g:HTMLPanel ui:field="container" styleName="ui-content ui-body-a">
		<div class="ui-corner-all ui-controlgroup ui-controlgroup-horizontal">
			<div class="ui-controlgroup-controls ">			
				<g:Anchor ui:field="mycontacts" styleName="ui-btn-active  ui-btn  ui-first-child">
					<span class="ui-btn-inner ui-corner-left">						
						<span class="ui-btn-text"><ui:safehtml from="{msg.myContacts}"/></span>												
					</span>
				</g:Anchor>								
				<g:Anchor ui:field="allcontacts" styleName="ui-btn ui-btn-up-c  ui-last-child">
					<span class="ui-btn-inner ui-corner-right ui-controlgroup-last">
						<span class="ui-btn-text"><ui:safehtml from="{msg.allContacts}"/></span>
					</span>
				</g:Anchor>
			</div>
		</div>
		<ul class="list-contacts">
			<li>
				<div class="user-name"><img alt="" src="aurore.allibe.jpg.png"/>Allibe Aurore</div>
				<a class="mail-link" href="mailto:aurore.allibe@silverpeas.com">aurore.allibe@silverpeas.com</a>
				<div class="tel"><a class="tel-link" href="tel:+33 6 88 76 75 56">+33 6 88 76 75 56 </a> | <a class="tel-link" href="tel:+33 4 76 09 31 61">+33 4 76 09 31 61</a>  | 
				<a class="tel-link" href="tel:+33 4 76 07 27 73">+33 4 76 07 27 73</a></div>
			</li>
		</ul>
	</g:HTMLPanel>
</ui:UiBinder> 